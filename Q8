def product_of_array_elements(arr):
    result = 1
    for num in arr:
        result *= num
    return result

# Test the function with the examples
arr1 = [1, 2, 3, 4, 5]
arr2 = [1, 6, 3]

print(product_of_array_elements(arr1))  # Output: 120
print(product_of_array_elements(arr2))  # Output: 18
